{
  "name": "gatbsy-wordpress-starter",
  "private": true,
  "description": "A Gatsby/Wordpress starter with Custom Post Types and Styled Components.",
  "version": "0.1.0",
  "author": "Midcoast Collective <info@midcoast.io>",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.52",
    "gatsby-cli": "^2.12.90",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.30",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-source-wordpress-experimental": "^1.3.8",
    "gatsby-transformer-sharp": "^2.5.13",
    "prettier": "2.1.1"
  },
  "keywords": [
    "gatsby",
    "wordpress",
    "typescript",
    "styled-components",
    "gatsby-cloud",
    "netlify",
    "jest",
    "react-testing-library"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --log-pages",
    "prod": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "packages": "yarn upgrade-interactive --latest",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arnonate/gatsby-wordpress-starter"
  },
  "bugs": {
    "url": "https://github.com/arnonate/gatbsy-wordpress-starter/issues"
  }
}
